{
  "name": "@tib/testlab",
  "version": "3.4.1",
  "description": "A collection of test utilities we use to write LoopBack tests.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "tib-tsc",
    "clean": "tib-clean tib-testlab*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "tib-mocha \"dist/__tests__/**/*.js\"",
    "verify": "npm pack && tar xf tib-testlab*.tgz && tree package && npm run clean"
  },
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@hapi/shot": "^5.0.0",
    "@types/express": "^4.17.7",
    "@types/fs-extra": "^9.0.1",
    "@types/shot": "^4.0.0",
    "@types/sinon": "^9.0.4",
    "@types/supertest": "^2.0.10",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "oas-validator": "^4.0.6",
    "should": "^13.2.3",
    "sinon": "^9.0.2",
    "supertest": "^4.0.2",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@tib/build": "^6.3.1",
    "@tib/eslint-config": "^8.2.1",
    "@types/node": "^14.0.25"
  },
  "files": [
    "README.md",
    "dist",
    "fixtures",
    "should-as-function.d.ts",
    "src",
    "!*/__tests__"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tibjs/framework.git",
    "directory": "packages/testlab"
  }
}
